<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-evergage-bnext-integration" version="0.0.1"
	xmlns="http://apache.org/cordova/ns/plugins/1.0"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<name>EvergageBnextIntegration</name>
	<js-module name="EvergageBnextIntegration" src="www/EvergageBnextIntegration.js">
		<clobbers target="EvergageBnextIntegration" />
	</js-module>
	<platform name="android">
		<config-file parent="/*" target="res/xml/config.xml">
			<feature name="EvergageBnextIntegration">
				<param name="android-package" value="cordova-plugin-evergage-bnext-integration.EvergageBnextIntegration" />
			</feature>
		</config-file>
		<config-file parent="/*" target="AndroidManifest.xml" />
		<source-file src="src/android/EvergageBnextIntegration.java" target-dir="src/mx/bnext/EvergageBnextIntegration" />
	</platform>
	<platform name="ios">
		<config-file parent="/*" target="config.xml">
			<feature name="EvergageBnextIntegration">
				<param name="ios-package" value="EvergageBnextIntegration" />
			</feature>
		</config-file>
		<source-file src="src/ios/EvergageBnextIntegration.m" />
	</platform>
</plugin>